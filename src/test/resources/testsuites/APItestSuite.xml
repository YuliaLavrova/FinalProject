<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name = "APISuite" thread-count="3" parallel="methods">
    <!--    <parameter name="key" value="value"></parameter>-->
    <listeners>
        <listener class-name="org.example.retry.AnnotationTransformer"/>
    </listeners>
    <test name = "APITest">
        <classes>
            <class name = "org.example.apiTest.APITest" >
                <methods>
                    <exclude name="searchTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>